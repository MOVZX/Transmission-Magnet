{
    "manifest_version": 3,
    "name": "Magnet to Transmission",
    "version": "1.1",
    "description": "Send clicked magnet links directly to Transmission.",
    "author": "MOVZX",
    "homepage_url": "https://github.com/MOVZX/Transmission-Magnet",
    "permissions": ["scripting", "notifications", "storage"],
    "icons": {
        "128": "icon.png"
    },
    "action": {
        "default_popup": "options.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_start"
        }
    ]
}
